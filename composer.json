{
    "name": "guardian360/resonance",
    "type": "library",
    "description": "Resonance aims to make performing HTTP requests and getting responses extremely simple.",
    "version": "2.0.0",
    "homepage": "https://github.com/guardian360/resonance",
    "license": "MIT",
    "authors": [
        {
            "name": "Niek den Breeje",
            "email": "n.denbreeje@guardian360.nl"
        }
    ],
    "require": {
        "php": "^7.3|^8",
        "guzzlehttp/guzzle": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/var-dumper": "^4|^5|^6|^7"
    },
    "autoload": {
        "psr-4": {
            "Guardian360\\Resonance\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Guardian360\\Resonance\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
